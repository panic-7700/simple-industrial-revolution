	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeAmmo_58Minieball_FMJ</defName>
		<label>make .58 Ball cartridge x200</label>
		<description>Craft 200 .58 Ball cartridges.</description>
		<jobString>Making .58 Ball cartridges.</jobString>
		<researchPrerequisites>
			<li>CE_Gunpowder</li>
		</researchPrerequisites>		
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>30</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>		
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_58Minieball_FMJ>200</Ammo_58Minieball_FMJ>
		</products>
		<workAmount>3000</workAmount>
	</RecipeDef>
	
