	<!--Line Infantry
	: Low to Normal Quality : Musket -->

	<PawnKindDef Name="ei_InfantryLineBase" ParentName="EarlyInd_BasePawn" Abstract="True">
		<combatPower>40</combatPower>
		<isFighter>true</isFighter>
		<canBeSapper>false</canBeSapper>
		<fixedGender>Male</fixedGender>
		<!-- uses default poor quality and lower combat power. worst aim. -->
		<itemQuality>Poor</itemQuality>
		<skills>
			<li>
				<skill>Shooting</skill>
				<range>3~5</range>
			</li>
		</skills>
		<apparelTags>
			<li>ei_apparel_line</li>
		</apparelTags>
		<specificApparelRequirements>
			<li>
				<apparelLayer>Webbing</apparelLayer>
				<requiredTag>ei_apparel_line</requiredTag>
			</li>
			<li>
				<bodyPartGroup>UpperHead</bodyPartGroup>
				<requiredTag>ei_apparel_line</requiredTag>
			</li>
			<li>
				<bodyPartGroup>Torso</bodyPartGroup>
				<requiredTag>ei_apparel_line</requiredTag>
			</li>
		</specificApparelRequirements>
		<apparelMoney>
			<min>1000</min>
			<max>1000</max>
		</apparelMoney>
		<weaponTags>
			<li>ei_Musket</li>
		</weaponTags>
		<weaponMoney>
			<min>200</min>
			<max>400</max>
		</weaponMoney>
	</PawnKindDef>

	<PawnKindDef ParentName="ei_InfantryLineBase">
		<defName>ei_exp_Infantry_reserve</defName>
